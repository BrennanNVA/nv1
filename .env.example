# Discord Webhooks (REQUIRED)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Database Connection (override config.toml if needed)
TIMESCALE_HOST=localhost
TIMESCALE_PORT=5432
TIMESCALE_DB=nova_aetus
TIMESCALE_USER=postgres
TIMESCALE_PASSWORD=postgres

# Alpaca API Credentials (REQUIRED for data fetching)
# Get your API keys from: https://app.alpaca.markets/paper/dashboard/overview
# Pro subscription uses: https://api.alpaca.markets (already configured in config.toml)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here

# Ollama Configuration (optional overrides)
# OLLAMA_HOST=http://localhost:11434
# OLLAMA_MODEL=llama3

# System Configuration
# LOG_LEVEL=INFO
# ENVIRONMENT=development
